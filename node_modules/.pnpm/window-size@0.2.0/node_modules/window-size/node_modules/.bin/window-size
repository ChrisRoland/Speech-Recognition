#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Dev/_JS-30/20 - Speech Detection/node_modules/.pnpm/window-size@0.2.0/node_modules/window-size/node_modules:/Users/mac/Dev/_JS-30/20 - Speech Detection/node_modules/.pnpm/window-size@0.2.0/node_modules:/Users/mac/Dev/_JS-30/20 - Speech Detection/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/Dev/_JS-30/20 - Speech Detection/node_modules/.pnpm/window-size@0.2.0/node_modules/window-size/node_modules:/Users/mac/Dev/_JS-30/20 - Speech Detection/node_modules/.pnpm/window-size@0.2.0/node_modules:/Users/mac/Dev/_JS-30/20 - Speech Detection/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
